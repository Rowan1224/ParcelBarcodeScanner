<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape>
            <solid
                android:color="#8BBC6D"/>
            <stroke
                android:color="#b3000000"
                android:width="1.5dp"/>
            <corners
                android:radius="6dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#A7CC90"/>
            <stroke
                android:color="#5d6d7e"
                android:width="1dp"/>
            <corners
                android:radius="6dp"/>
        </shape>
    </item>
</selector>