<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape>
            <solid
                android:color="#34495e"/>
            <stroke
                android:color="#000"
                android:width="1.5dp"/>
            <corners
                android:radius="5dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#5d6d7e"/>
            <stroke
                android:color="#AAAAAA"
                android:width="1dp"/>
            <corners
                android:radius="4dp"/>
        </shape>
    </item>
</selector>